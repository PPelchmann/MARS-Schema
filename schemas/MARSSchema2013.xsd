    <xs:attribute name="notify_email_address_cc" type="xs:string" use="optional" />
    <xs:attribute name="notify_email_subject" type="xs:string" use="optional" />
    <xs:attribute name="notify_email_body" type="xs:string" use="optional" />
  </xs:complexType>
  
