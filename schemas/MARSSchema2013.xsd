<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://mars-o-matic.com" 
           xmlns:aae="http://mars-o-matic.com"
           elementFormDefault="qualified">
    
  <xs:include schemaLocation="http://mars-o-matic.com/2013/MARSSchema.xsd"/>
  
  <xs:simpleType name="number-or-empty">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]*" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="esx-commands">
    <xs:restriction base="xs:string">
      <xs:enumeration value="cpu" />
      <xs:enumeration value="mem" />
      <xs:enumeration value="net" />
      <xs:enumeration value="io" />
      <xs:enumeration value="runtime" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ssl-enabled">
    <xs:annotation>
      <xs:documentation>This option is for https requests.
        Possible values:
        "true" or "false". default: "false"
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:boolean" />
  </xs:simpleType>

  <xs:complexType name="CheckBaseType">
    <xs:attribute name="info" type="xs:string" use="optional" />
    <xs:attribute name="MonitorNodeID" type="aae:aaeID" use="optional" />
  </xs:complexType>

  <xs:complexType name="CheckWithEmail">
    <xs:attribute name="info" type="xs:string" use="optional" />
